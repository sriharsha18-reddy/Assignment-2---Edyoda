   "source": [
    "def last(n):\n",
    "    return n[-1]\n",
    "\n",
    "def sort(tuples):\n",
    "    return sorted(tuples, key=last)\n",
    "\n",
    "x=[(2,5),(1,2),(4,4),(2,3),(2,1)]\n",
    "print(\"Sorted List is :\")\n",
    "print(sort(x))        "
